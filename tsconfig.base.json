{
  "compilerOptions": {
    "target": "ESNext"
  , "module": "CommonJS"
  , "strict": true
  , "esModuleInterop": true
  , "noUnusedLocals": true
  , "noUnusedParameters": true
  , "baseUrl": "."
  , "paths": {
      "@src/*": ["src/*"]
    , "@cedarwood/*": ["src/cedarwood/*"]
    , "@daachorse/*": ["src/daachorse/*"]
    , "@test/*": ["__tests__/*"]
    }
  }
}
