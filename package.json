{
  "name": "ultra-nlp-js",
  "version": "0.4.3",
  "description": "A Node.js binding for ultra-nlp",
  "keywords": [
    "rust",
    "native"
  ],
  "files": [
    "lib",
    "native/src",
    "native/Cargo.lock",
    "native/Cargo.toml"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:BlackGlory/ultra-nlp-js.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "install": "neon build --release && rimraf native/target",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build",
    "clean": "run-p clean:*",
    "clean:rs": "neon clean",
    "clean:ts": "rimraf lib",
    "build": "run-p build:*",
    "build:rs": "neon build --release",
    "build:ts": "run-s build:ts:*",
    "build:ts:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib",
    "build:ts:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "bench": "./benches/benchmark.sh",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/go": "^1.1.2",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/jest": "^29.2.3",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "extra-benchmark": "^0.1.1",
    "extra-filesystem": "^0.4.8",
    "husky": "4",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "pretty-bytes": "5",
    "return-style": "^1.0.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.3",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@blackglory/prelude": "^0.1.8",
    "extra-lazy": "^1.3.1",
    "justypes": "^3.1.2",
    "neon-cli": "^0.10.1",
    "rimraf": "^3.0.2"
  }
}
